{
  "multipart": [
    {
      "apply": {
        "model": "factory_expansion:block/girder/black/horizontal"
      },
      "when": {
        "x": "true"
      }
    },
    {
      "apply": {
        "model": "factory_expansion:block/girder/black/vertical"
      },
      "when": {
        "y": "true"
      }
    },
    {
      "apply": {
        "model": "factory_expansion:block/girder/black/horizontal",
        "y": 90
      },
      "when": {
        "z": "true"
      }
    },
    {
      "apply": {
        "model": "factory_expansion:block/girder/black/midsection"
      },
      "when": {
        "y": "false",
        "z": "false",
        "x": "true"
      }
    },
    {
      "apply": {
        "model": "factory_expansion:block/girder/black/midsection",
        "y": 90
      },
      "when": {
        "y": "false",
        "z": "true"
      }
    }
  ]
}